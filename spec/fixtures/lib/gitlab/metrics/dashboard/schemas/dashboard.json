{
 "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "dashboard",
    "priority",
    "panel_groups"
  ],
  "properties": {
    "dashboard": { "type": "string" },
    "priority": { "type": "number" },
    "panel_groups": {
      "type": "array",
      "items": { "$ref": "panel_groups.json" }
    },
    "templating": { "$ref": "templating.json" },
    "links": { "$ref": "links.json" }
  },
  "additionalProperties": false
}
