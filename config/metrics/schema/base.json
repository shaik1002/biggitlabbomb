{
  "type": "object",
  "required": [
    "key_path",
    "description",
    "value_type",
    "status",
    "product_group",
    "time_frame",
    "data_source",
    "distribution",
    "tier",
    "data_category",
    "milestone"
  ],
  "additionalProperties": false,
  "properties": {
    "key_path": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "product_group": {
      "$ref": "product_groups.json"
    },
    "value_type": {
      "type": "string",
      "enum": [
        "string",
        "number",
        "boolean",
        "object"
      ]
    },
    "status": {
      "type": [
        "string"
      ],
      "enum": [
        "active",
        "removed",
        "broken"
      ]
    },
    "milestone": {
      "type": [
        "string"
      ],
      "pattern": "^<?[0-9]+\\.[0-9]+$"
    },
    "milestone_removed": {
      "type": [
        "string"
      ],
      "pattern": "^<?[0-9]+\\.[0-9]+$"
    },
    "introduced_by_url": {
      "type": [
        "string",
        "null"
      ],
      "format": "uri"
    },
    "removed_by_url": {
      "type": [
        "string",
        "null"
      ],
      "format": "uri"
    },
    "repair_issue_url": {
      "type": [
        "string"
      ],
      "format": "uri"
    },
    "options": {
      "type": "object"
    },
    "time_frame": {
      "type": "string",
      "enum": [
        "7d",
        "28d",
        "all",
        "none"
      ]
    },
    "data_source": {
      "type": "string",
      "enum": [
        "database",
        "redis",
        "redis_hll",
        "prometheus",
        "system",
        "license",
        "internal_events"
      ]
    },
    "data_category": {
      "type": "string",
      "enum": [
        "operational",
        "optional",
        "subscription",
        "standard"
      ]
    },
    "instrumentation_class": {
      "type": "string",
      "pattern": "^(([A-Z][a-z]+)+::)*(([A-Z]+[a-z]+)+)$"
    },
    "distribution": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "ee",
          "ce"
        ]
      }
    },
    "performance_indicator_type": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "gmau",
          "smau",
          "paid_gmau",
          "umau",
          "customer_health_score",
          "devops_report"
        ]
      }
    },
    "tier": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "free",
          "starter",
          "premium",
          "ultimate",
          "bronze",
          "silver",
          "gold"
        ]
      }
    },
    "value_json_schema": {
      "type": "string"
    },
    "product_category": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "events": {
    }
  },
  "allOf": [
    {
      "$ref": "internal_events.json"
    },
    {
      "$ref": "redis_hll.json"
    },
    {
      "$ref": "redis.json"
    },
    {
      "$ref": "status.json"
    }
  ]
}
